{
  "name": "cimovieflix",
  "version": "1.0.0",
  "description": "Website for movie",
  "main": "server.js",
  "engine": {
    "node": "8.10.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' --c 'red,green' \"nodemon server.js\" \"npm run client\" \"npm run start --prefix client\""
  },
  "author": "Nadhrah",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "color": "^3.1.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongoose": "^5.9.24",
    "react-animated-slider": "^2.0.0",
    "react-redux": "^5.1.2",
    "react-scripts": "^3.4.1",
    "require-stack": "^1.0.2",
    "saslprep": "^1.0.3",
    "supports": "^0.1.1",
    "supports-color": "^7.1.0",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^1.19.4"
  }
}
